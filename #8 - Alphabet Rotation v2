#include<bits/stdc++.h>
using namespace std;

map <string ,int> m;
string v[100000] ,aux;
int n,r;

int main()
{
    cin>>n;
    for(int i=0;i<n;++i)
    {
        cin>>v[i];
        int dif = v[i][0] - 'a';
        for(auto& u : v[i]) {
            r  = u - 'a' - dif;
            if(r < 0) r += 26;
            u = (char)(r + 'a');
        }
        m[v[i]]++;
    }

    for(int i=0;i<n;++i)
    {
        if(m[v[i]]>1)
            cout<<"1\n";
        else
            cout<<"0\n";
    }

    return 0;
}
